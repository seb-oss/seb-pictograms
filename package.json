{
  "name": "seb-pictograms-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "seb-pictograms:data": "json-to-scss projects/seb-pictograms/data.json",
    "seb-pictograms:compile": "node-sass --output-style=compressed projects/seb-pictograms/pictograms.scss projects/seb-pictograms/pictograms.css",
    "seb-pictograms:docs": "copyfiles README.md LICENSE.md projects/seb-pictograms",
    "seb-pictograms:pack": "npm run seb-pictograms:data && npm run seb-pictograms:compile && npm run seb-pictograms:docs",
    "github-pages:deploy": "env-cmd .env npx angular-cli-ghpages --dir=dist/seb-pictograms-app --repo=https://GH_TOKEN@github.com/sebgroup/seb-pictograms.git --no-silent",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "cd projects/seb-pictograms && semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ng-bootstrap/ng-bootstrap": "^5.3.1",
    "@sebgroup/bootstrap": "^2.4.1",
    "core-js": "^2.5.4",
    "imagemin": "^7.0.1",
    "imagemin-svgo": "^8.0.0",
    "rxjs": "~6.6.7",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular/cli": "~8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^5.0.1",
    "copyfiles": "^2.1.0",
    "env-cmd": "^8.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-to-scss": "^1.3.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "semantic-release": "^17.1.1",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
